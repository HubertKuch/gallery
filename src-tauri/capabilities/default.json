{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "core:window:allow-close",
    "dialog:default",
    "fs:default",
    "fs:allow-app-read",
    "fs:allow-home-read"
  ]
}